
@model  Joker.Models.JokeViewModel

@{
    ViewBag.Title = "Profile";
}

<h2>Profile</h2>

@foreach (var joke in Model.JokeList)
{
    <p>@joke.JokeTitle</p>
}

<h3>People you follow</h3>
@foreach (var person in Model.JokeList)
{
    <p>
        @person.ApplicationUser.UserName
        @person.ApplicationUser.Following.FirstOrDefault()
    </p>
}
